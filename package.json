{
  "name": "bull-board",
  "description": "Bull queue UI for inspecting jobs",
  "keywords": [
    "bull",
    "redis",
    "queue"
  ],
  "main": "index.js",
  "private": false,
  "version": "0.5.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vcapretz/bull-board.git"
  },
  "bugs": {
    "url": "https://github.com/vcapretz/bull-board/issues"
  },
  "scripts": {
    "prepublishOnly": "rm -rf ./static && yarn build",
    "test": "node ./dockest.js",
    "build": "cross-env NODE_ENV=production webpack",
    "build:watch": "cross-env NODE_ENV=production webpack --watch",
    "start": "node example.js",
    "start:dev": "concurrently \"yarn build:watch\" \"yarn start\"",
    "start:example": "yarn build && yarn start",
    "format": "prettier --write --ignore-path .gitignore \"**\""
  },
  "dependencies": {
    "bull": "^3.12.1",
    "cross-env": "^6.0.3",
    "date-fns": "2.8.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "pretty-bytes": "^5.1.0",
    "ramda": "^0.26.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-highlight": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.2",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "dockest": "^2.0.0-alpha.1",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react-dev-utils": "^9.1.0",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
